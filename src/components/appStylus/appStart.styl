.appStart
  padding: 0 0!important
  >div
    padding: 0 0 0
    text-align: center
    img
      width: 100%
  .appName 
    position: absolute
    top: 3.125rem 
    left: 20%
    width: 60%
    border: 1px solid #9B7D58
    background: rgba(0,0,0,0.5)
    font-size: 1.5625rem
    color: #fff 
    text-align: center
    height: 2.25rem
    line-height: 2.25rem
  .appTips
    margin-top: 1rem
    font-size: 0.875rem
    color: #9B7D58
  .datetime
    color: #9B7D58
    font-size: 0.875rem
    padding: 3px
    border: 1px solid #9B7D58
    width: 83%
    margin: 1.875rem auto 0.75rem
    >div
      border: 1px dotted #9B7D58
      >div 
        display: inline-block 
        width: 49%
        padding: 0.625rem
        height: 4.5rem
        vertical-align: middle
    .dateweek
      font-size: 2rem
      line-height: 3.375rem
    .dateyear 
      >div 
        font-size: 1.125rem
      >div:nth-child(1)
        font-size: 1.375rem
        padding-bottom: 5px
  .ivu-btn-primary
    width: 3.5rem
    height: 3.5rem
    padding: 0
    display: inline-block
    position: absolute
    left: calc(50% - 1.75rem)
    top: 88vh